apiVersion: v1
baseDomain: {{ base_domain.msg }}
compute:
- hyperthreading: Enabled
  name: worker
  replicas: {{ n_workers }}
controlPlane:
  hyperthreading: Enabled
  name: master
  replicas: {{ n_masters }}
metadata:
  name: {{ ocp_domain.msg }}
networking:
  clusterNetwork:
  - cidr: {{ cluster_network }}
    hostPrefix: {{ cluster_network_subnet }}
  networkType: OpenShiftSDN
  serviceNetwork:
  - 172.30.0.0/16
platform:
 none: {}
fips: false
sshKey: {{ ssh_pub_key.stdout }}
